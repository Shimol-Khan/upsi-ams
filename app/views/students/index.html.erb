<p id="notice"><%= notice %></p>

<h1>Students</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Matric</th>
      <th>Name</th>
      <th>Student status</th>
      <th>Study level</th>
      <th>Current semester</th>
      <th>Student type</th>
      <th>College</th>
      <th>Ic number</th>
      <th>Passport number</th>
      <th>Permanent address</th>
      <th>Current address</th>
      <th>Country</th>
      <th>Gender</th>
      <th>Date of birth</th>
      <th>Mobile number</th>
      <th>Blood type</th>
      <th>Race</th>
      <th>Religion</th>
      <th>Marital status</th>
      <th>Bank</th>
      <th>Bank account</th>
      <th>Sponsor</th>
      <th>Siswa email</th>
      <th>Personal email</th>
      <th>Cgpa 1</th>
      <th>Cgpa 2</th>
      <th>Cgpa 3</th>
      <th>Cgpa 4</th>
      <th>Cgpa 5</th>
      <th>Cgpa 6</th>
      <th>Cgpa 7</th>
      <th>Cgpa 8</th>
      <th>Faculty</th>
      <th>Program</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.user_id %></td>
        <td><%= student.matric_id %></td>
        <td><%= student.name %></td>
        <td><%= student.student_status %></td>
        <td><%= student.study_level %></td>
        <td><%= student.current_semester %></td>
        <td><%= student.student_type %></td>
        <td><%= student.college %></td>
        <td><%= student.ic_number %></td>
        <td><%= student.passport_number %></td>
        <td><%= student.permanent_address %></td>
        <td><%= student.current_address %></td>
        <td><%= student.country %></td>
        <td><%= student.gender %></td>
        <td><%= student.date_of_birth %></td>
        <td><%= student.mobile_number %></td>
        <td><%= student.blood_type %></td>
        <td><%= student.race %></td>
        <td><%= student.religion %></td>
        <td><%= student.marital_status %></td>
        <td><%= student.bank %></td>
        <td><%= student.bank_account %></td>
        <td><%= student.sponsor %></td>
        <td><%= student.siswa_email %></td>
        <td><%= student.personal_email %></td>
        <td><%= student.cgpa_1 %></td>
        <td><%= student.cgpa_2 %></td>
        <td><%= student.cgpa_3 %></td>
        <td><%= student.cgpa_4 %></td>
        <td><%= student.cgpa_5 %></td>
        <td><%= student.cgpa_6 %></td>
        <td><%= student.cgpa_7 %></td>
        <td><%= student.cgpa_8 %></td>
        <td><%= student.faculty %></td>
        <td><%= student.program %></td>
        <td><%= link_to 'Show', student %></td>
        <td><%= link_to 'Edit', edit_student_path(student) %></td>
        <td><%= link_to 'Destroy', student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student', new_student_path %>
